{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "stream",
  "tasks": {
    "start": {
      "dependsOn": ["^start"],
      "persistent": true,
      "cache": false,
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "dev": {
      "dependsOn": ["^dev", "db:generate"],
      "cache": false,
      "persistent": true
    },
    "db:generate": {},
    "db:push": {},
    "lint": {},
    "type-check": {},
    "format": {}
  }
}
